@page
@model WebApp.Pages.AssignaturesModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
  Layout = "../shared/_AdminMenuLayout.cshtml";
}

<div class="jumbotron">
  <h1 class="display-3">Assignatures</h1>
  <table class="table">
    <thead class="thead-inverse">
      <tr><th>School Assignatures</th></tr>
    </thead>
    <tbody>
    @foreach(var assignature in Model.Assignatures)
    {
      <tr><td>@assignature.IdTeacher teaches @assignature.IdSubject to @assignature.IdGroup</td></tr>
    }
    </tbody>
  </table>
</div>