@page
@model WebApp.Pages.SubjectsModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
  Layout = "../shared/_AdminMenuLayout.cshtml";
}

<div class="jumbotron">
  <h1 class="display-3">Subjects</h1>
  <table class="table">
    <thead class="thead-inverse">
      <tr><th>School Subjects</th></tr>
    </thead>
    <tbody>
    @foreach(var subject in Model.Subjects)
    {
      <tr><td>@subject.Name</td></tr>
    }
    </tbody>
  </table>
</div>