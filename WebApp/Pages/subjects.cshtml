@page
@model WebApp.Pages.SubjectsModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<div class="row">
  <h1 class="display-2">Subjects</h1>
  <table class="table">
    <thead class="thead-inverse">
      <tr><th>School Subjects</th></tr>
    </thead>
    <tbody>
    @foreach(string name in Model.Subjects)
    {
      <tr><td>@name</td></tr>
    }
    </tbody>
  </table>
</div>
